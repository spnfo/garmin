<layouts>
<layout id="InitializeLayout">
	<bitmap id="spnfo_logo" x="center" y="30" filename="../drawables/spnfo-purple-light-tiny.png" />
	<label 
		id="topLabel" 
		x="center" 
		y="100" 
		justification="Gfx.TEXT_JUSTIFY_CENTER"
		font="Gfx.FONT_SMALL" 
		text="@Strings.initializeTopLabel" 
		color="Gfx.COLOR_WHITE" 
	/>
	<label
		id="codeLabel"
		x="center"
		y="180"
		justification="Gfx.TEXT_JUSTIFY_CENTER"
		font="@Fonts.roboto_mono_bold_large"
		text="@Strings.initializeCodeLabel"
		color="Gfx.COLOR_WHITE"
	/>
	<label
		id="bottomLabel"
		x="center"
		y="280"
		justification="Gfx.TEXT_JUSTIFY_CENTER"
		font="Gfx.FONT_SMALL"
		text="@Strings.initializeBottomLabel"
		color="Gfx.COLOR_WHITE"
	/>
</layout>
<layout id="HomeScreenLayout">
	<bitmap id="spnfo_logo" x="center" y="30" filename="../drawables/spnfo-purple-light-tiny.png" />
	<label 
		id="topLabel"
		x="center"
		y="100"
		font="Gfx.FONT_SMALL"
		color="Gfx.COLOR_WHITE"
		justification="Gfx.TEXT_JUSTIFY_CENTER"
		text="@Strings.instructionsLabel1"
	/>
	<label 
		id="centerLabel"
		x="center"
		y="center"
		font="@Fonts.roboto_mono_bold_large"
		color="Gfx.COLOR_WHITE"
		justification="Gfx.TEXT_JUSTIFY_CENTER"
	/>
	<label 
		id="bottomLabel"
		x="center"
		y="250"
		font="Gfx.FONT_SMALL"
		color="Gfx.COLOR_WHITE"
		justification="Gfx.TEXT_JUSTIFY_CENTER"
		text="@Strings.instructionsLabel2"
	/>
</layout>
<layout id="RaceCompletedLayout">
	<bitmap id="spnfo_logo" x="center" y="30" filename="../drawables/spnfo-purple-light-tiny.png" />
	<label 
		id="centerLabel"
		x="center"
		y="center"
		font="Gfx.FONT_SMALL"
		color="Gfx.COLOR_WHITE"
		text="@Strings.raceCompletedLabel"
		justification="Gfx.TEXT_JUSTIFY_CENTER"
	/>
</layout>
